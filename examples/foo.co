fun printu32(u32) void

type Stuff {
  x int
}

type Thing {
  x, y, z u32 = 3, 4, 5
  stuff ?Stuff
}

fun Thing.drop(mut this) {
  printu32(2)
}

fun Stuff.drop(mut this) {
  printu32(3)
}

fun foo(x, y int) int {
  printu32(1)
  let a = Thing(x: u32(x))
  if x > 1 {
    a.stuff = Stuff()
  }
  x * y * int(a.z)
}
