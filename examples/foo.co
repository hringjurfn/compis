type Thing {
  x, y, z u32 = 3, 4, 5
  pretty bool
  f1 fun(int)int
}

fun Thing.foo(x int) int {
  x * x
}

fun bar(x, y int) int {
  let a = Thing(u32(x), pretty: true, y: 5)
  // a.f1(x)
  int(a.x) + a.foo(x) * y
}

fun foo(x, y int) int {
  var z = x + y * int(2)
  bar(z, y: 6)
}

